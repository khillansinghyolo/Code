
public class test {

	
	public static void main(String[] args) {
		
		String in="rohit singh";
		char []ss=in.toCharArray();
		String rev="";
		int s=in.length();
		for (int i = s-1; i >0; i--) 
		{
		rev=rev+ss[i];	
		System.out.println(rev);
		}
		String sss[]=rev.split(" ");	
		int ssss=sss.length;
	String s4="";
	for (int j = ssss-1; j >0; j--) {
		s4=s4+sss[j];
		
		System.out.println(s4);
	}
		
		
		
	}
}
